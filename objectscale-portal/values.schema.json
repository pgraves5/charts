{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "required": [
    "accept_eula"
  ],
  "properties": {
    "accept_eula": {
     "type": "string",
     "title": "Date revision of the EULA in the format DDmmmYYYY where mmm is Jan Feb etc...",
     "examples": [
	      "20Jan21",
        "03Mar20"
     ], 
     "pattern": "^(([0-9])|([0-2][0-9])|([3][0-1]))(Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec)20[2-9][0-9]|[2-9][1-9][0-9][0-9]$"
    }
  }
}
